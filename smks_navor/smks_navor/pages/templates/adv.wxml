<template name="swiperlist">
  <view class="swiper">
    <swiper indicator-dots="true" autoplay="true" interval="3000" duration="1200">
      <swiper-item wx:for="{{list}}" wx:key="{{index}}">
       <navigator style="width:100%;height:100%"
        catchtap='goToUrl' url="{{item.url||item.link}}" open-type="{{item.openType||item.opentype}}"
        data-url="{{item.url||item.link}}" 
        data-linktype="{{item.linktype}}" data-opentype="{{item.opentype}}"
        data-appid="{{item.appid}}"
        data-appparam="{{item.appparam}}"
        data-id="{{item.id}}"
        >
          <image  class="nav-icon" mode="aspectFill" src="{{item.icon||item.thumb}}" />
          <label>{{item.name||item.advname}}</label>
        </navigator>
      </swiper-item>
    </swiper>
  </view>
</template>
<template name="icon3list">
  <view class="nav">
    <block wx:for="{{list}}" wx:key="{{index}}">
      <navigator class="nav-item {{(index + 1) % 3 == 0 ? 'nav-item-no-border' : ''}}" hover-class="navigator-hover" 
      catchtap='goToUrl' url="{{item.url||item.link}}" open-type="{{item.openType||item.opentype}}"
      data-url="{{item.url||item.link}}" 
      data-linktype="{{item.linktype}}" data-opentype="{{item.opentype}}"
      data-appid="{{item.appid}}"
      data-appparam="{{item.appparam}}"
      data-id="{{item.id}}"
      >
        <image wx:if="{{item.icon||item.thumb}}" class="nav-icon" src="{{item.icon ||item.thumb}}" />
        <view wx:if="{{item.name||item.advname2}}" class="nav-title">{{item.name||item.advname}}</view>
      </navigator>
    </block>
  </view>
</template>
