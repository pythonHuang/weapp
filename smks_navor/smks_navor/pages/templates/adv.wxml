<template name="contentlist">
  <view class="commend">
    <block wx:for="{{navbars}}"  wx:for-item="subItem"  wx:key="{{index}}">
      <view wx:if="{{subItem.navname||subItem.name}}" class="commend-header-title">{{subItem.navname||subItem.name}}</view>
      <view class="commend-column">
          <block wx:for="{{subItem.advs}}" wx:key="{{index}}">
        <navigator class="commend-item"  hover-class="navigator-hover"
        catchtap='goToUrl' url="{{item.url||item.link}}" open-type="{{item.openType||item.opentype}}"
      data-url="{{item.url||item.link}}" 
      data-linktype="{{item.linktype}}" data-opentype="{{item.opentype}}"
      data-appid="{{item.appid}}"
      data-appparam="{{item.appparam}}"
      data-id="{{item.id}}"
        >
          <image wx:if="{{item.icon||item.thumb}}" class="commend-thumb" src="{{item.icon ||item.thumb}}"  style="{{item.height?'':(item.height+'rpx')}}" />
          <view wx:if="{{item.title||item.name||item.advname}}" class="commend-title">{{item.title||item.name||item.advname}}</view>
        </navigator>
        <block wx:if="{{item.adid}}">
          <ad unit-id="{{item.adid}}">
          </ad>
        </block>
        </block>
      </view>
      <block wx:if="{{subItem.adid}}">
          <ad unit-id="{{subItem.adid}}">
          </ad>
        </block>
    </block>
  </view>
</template>
